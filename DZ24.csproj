// Задача 24: Найти кубы чисел от 1 до N

Console.WriteLine("Введите число: ");
int num = int.Parse(Console.ReadLine());
Console.WriteLine("");

int x = 1;
int y = 1;

while (x <= num)
{
    Console.Write(x + "\t");
    x++;
}
Console.WriteLine();
while (y <= num)
{
    Console.Write(y*y*y + "\t");
    y++;
}
Console.WriteLine();